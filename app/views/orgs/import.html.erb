<% set_page_title( "Import users" ) %>

<h1>Import Users</h1>

<%= error_messages_for 'org' %>

<%= render :partial => "csvformat"%>

<% form_for :dump, :url => { :action => "import_file" }, :html => { :multipart => true } do |form| %>
   <table>
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
     <td >
       <%= form.file_field :file %>
     </td>
   </tr>
   <tr>
     <td></td>
     <td>
       <%= submit_tag 'Submit' %>
     </td>
   </tr>
 </table>
<% end %>
